#include<stdio.h>
int main()
{
    int a[2][2],i,j;
    printf("\n enter the elements of a given matrix \n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            scanf("%d\t" , &a[i][j]);
        }
    }
    printf("\n....Given matrix....\n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            printf("%d\t" , a[i][j]);
        }
        printf("\n");
    }
    printf("\n................\n");
    printf("\n Transpose of a given matrix==> \n");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            printf("%d\t" , a[j][i]);
        }
        printf("\n");
    }
}
